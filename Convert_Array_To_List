using System;
using System.Collections.Generic;
using System.Linq;

namespace Practice2
{
    class Program
    {
        static void Main(string[] args)
        {

            //Convert INT array to String Array
            int[] NewArray = { 2, 3, 4, 5, 3, 9 };
            string[] myStrings = NewArray.Select(x => x.ToString()).ToArray();

            //Convert String Array to String LIST
            List<string> NewString = new List<string>(myStrings);

            NewString.ForEach(Console.WriteLine);
            
            Console.ReadLine();
        }
        
    }
}
